  a + b + c
}

addt :(int,int)=>int = { (a,b) =>
  a + b
}

def fib := {
  case 0 => 0
  case 1 => 1
  case n => fib(n-2) + fib(n-1)
}

def fib2 := { n =>
  n match {
    case 0 => 0
    case 1 => 1
    case n => fib2(n-2) + fib2(n-1)
  }
}

llor := {
  case 0,0 => a:=1 b:=2; a | b
  case a,b => a | b
}

main := {() =>
  printf("add 1 2 = %d\n" add(1 2))
  printf("addt 1,2 = %d\n" addt(1,2))
  printf("fib 10 = %d\n" fib(10))
  printf("llor %d\n" llor(1,2))
  a := 12
  c := 2
  b := c + a
  printf("%d\n" b)
}

main()

