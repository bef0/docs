k type | KInt(int) | KAdd(k, k)

def eval := {
  case KInt(i) => i
  case KAdd(a,b) =>
    a:=eval(a)
    b:=eval(b)
    a+b
}

printf("1+2=%d\n" eval(KAdd(KInt(1),KInt(2))))

